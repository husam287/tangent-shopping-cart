{
  "expo": {
    "name": "Tangent Shop",
    "slug": "tangent-shopping-cart",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "scheme": "tangentshop",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0d213d"
    },
    "updates": {
      "checkAutomatically": "ON_ERROR_RECOVERY",
      "url": "https://u.expo.dev/ab08f9dc-b2e1-4037-9939-06cdcc23589d"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hossam.tangentshop",
      "infoPlist": {
        "CFBundleAllowMixedLocalizations": true
      }
    },
    "android": {
      "package": "com.hossam.tangentshop",
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/adaptive-icon.png",
        "backgroundColor": "#0d213d"
      }
    },
    "locales": {
      "en": "./src/i18n/en.json",
      "ar": "./src/i18n/ar.json"
    },
    "plugins": [
      "expo-localization",
      "expo-notifications",
      "@mmomtchev/expo-android-localized-app-name"
    ],
    "experiments": {
      "tsconfigPaths": true
    },
    "extra": {
      "eas": {
        "projectId": "ab08f9dc-b2e1-4037-9939-06cdcc23589d"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
